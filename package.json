  "description": "Wollok IDE",
  "main": "index.js",
  "scripts": {
    "dev": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js --progress --colors",
    "build-dev": "./node_modules/webpack/bin/webpack.js",
    "build-prod": "NODE_ENV=production ./node_modules/webpack/bin/webpack.js -p --progress --colors",
    "prod": "NODE_ENV=production node index.js",
    "build-ssr": "NODE_ENV=production SSR_MODE=true ./node_modules/webpack/bin/webpack.js --progress --colors",
    "ssr": "NODE_ENV=production SSR_MODE=true node dist/server.js",
    "ssr:preprod": "NODE_ENV=preprod SSR_MODE=true node dist/server.js",
    "start": "node index.js",
    "test": "BABEL_DISABLE_CACHE=1 mocha test/helpers/browser.js test/*.spec.js",
    "test:live": "BABEL_DISABLE_CACHE=1 npm test -- -w",
    "run-aws": "npm run build-prod && node index.js"
  },
